# ---
# ### Plot 4: Duty Cycle Effect on Order Intensities
# 
# **Fixed parameters:** $N = 10$, $a = 0.20$ mm  
# **Variable:** Duty cycle $b/a = 1.0, 1/2, 1/3, 1/4, 1/5, 1/7$
# 
# **X-axis:** Order number $m$ (where $m = a\sin\theta/\lambda$)  
# This makes missing orders easier to identify at integer positions.

# %%
# Parameters for duty cycle study
N_duty = 10
a_duty_mm = 0.20
a_duty = a_duty_mm * 1e-3

# Duty cycle values
duty_cycles = [1.0, 1/2, 1/3, 1/4, 1/5, 1/7]

# Angular range
theta_duty = np.linspace(-np.pi/120, np.pi/120, 5000)

# Create subplots
fig, axes = plt.subplots(len(duty_cycles), 1, figsize=(10, 12), sharex=True)

for i, duty in enumerate(duty_cycles):
    # Calculate intensity
    intensity = fraunhofer_with_duty(theta_duty, N=N_duty, a=a_duty, duty_cycle=duty)
    
    # Convert angle to order number for x-axis
    s_values = (a_duty * np.sin(theta_duty)) / LAMBDA
    
    # Plot
    axes[i].plot(s_values, intensity, color='skyblue', linewidth=1.2)
    axes[i].set_ylabel('Intensity', fontsize=10)
    axes[i].grid(True, alpha=0.3)
    
    # Title showing duty cycle and predicted missing orders
    if duty == 1.0:
        title_str = f'$b/a = {duty:.1f}$ (all orders present)'
    else:
        # Calculate which orders should be missing
        frac = Fraction(duty).limit_denominator(10)
        missing_start = frac.denominator
        title_str = f'$b/a = {frac}$ (orders $\pm${missing_start}, $\pm${2*missing_start}, ... suppressed)'
    
    axes[i].set_title(title_str, fontsize=10, loc='left')
    
    # Mark integer order positions
    axes[i].axhline(y=0, color='k', linewidth=0.5, alpha=0.3)
    for m in range(-8, 9):
        axes[i].axvline(x=m, color='gray', linewidth=0.5, alpha=0.2, linestyle='--')

axes[-1].set_xlabel(r'Diffraction Order $m$ (dimensionless)', fontsize=11)
axes[-1].set_xlim(-8, 8)

fig.suptitle(f'Plot 4: Effect of Duty Cycle on Diffraction Order Intensities\n' +
             f'N = {N_duty}, $a$ = {a_duty_mm} mm, $\lambda$ = {LAMBDA_NM} nm',
             fontsize=12)

plt.tight_layout()
plt.show()

print("\nPlot 4 Analysis:")
